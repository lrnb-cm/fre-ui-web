{
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "@apollo/federation": "^0.25.1",
    "@apollo/gateway": "^0.28.3",
    "apollo-server": "^2.25.1",
    "apollo-server-express": "^2.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.1",
    "concurrently": "^6.4.0",
    "connect-redis": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^8.2.0",
    "ioredis": "^4.23.0",
    "module-alias": "^2.2.2",
    "pg": "^8.5.1",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.9.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.16",
    "@types/cors": "^2.8.10",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/graphql": "^14.5.0",
    "@types/ioredis": "^4.22.0",
    "@types/node": "^14.14.33",
    "@types/wait-on": "^5.3.1",
    "add": "^2.0.6",
    "bulma": "^0.9.2",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "yarn": "^1.22.10"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register"
  },
  "scripts": {
    "start": "nodemon",
    "client": "npm start --prefix client-enterprise",
    "dev": "concurrently \"npm run client\"   \"npm:start-services\"",
    "start-gateway": "nodemon ./src/gateway.ts",
    "start-service-container": "npm start ./src/container",
    "start-service-login": "npm start ./src/loginContainer",
    "start-services": "concurrently \"npm:start-service-*\"",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "_moduleAliases": {
    "~/*": "./src"
  }
}
