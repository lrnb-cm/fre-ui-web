{
  "name": "react",
  "version": "1.0.0",
  "description": "React example starter project",
  "keywords": [
    "react",
    "starter"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "axios": "^0.24.0",
    "bootstrap": "4.6.0",
    "classnames": "^2.3.1",
    "graphql": "15.5.0",
    "moment": "^2.29.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "react": "17.0.2",
    "react-awesome-modal": "^2.0.5",
    "react-bootstrap": "1.5.2",
    "react-dom": "17.0.2",
    "react-fontawesome": "^1.7.1",
    "react-progressbar.js": "^0.2.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^4.0.3",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@babel/runtime": "7.13.8",
    "sass": "^1.43.4",
    "typescript": "4.1.3"
  },
  "scripts": {
    "build-css": "sass --no-stdin ./src/scss --no-stdin ./src/common-strap --load-path ./src --load-path ./node_modules",
    "watch-css": "npm run build-css && sass --no-stdin ./src/scss --load-path ./src --load-path ./node_modules --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build && (mv build ../public || move build ../public)",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
